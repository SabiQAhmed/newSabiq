	else if(browserName.equals("Edge")) {
				if(StringUtils.isNotBlank(environment) && "AWS".equalsIgnoreCase(environment)){
					System.setProperty("wdm.proxy", "proxy.ebiz.verizon.com:80");
					System.out.println("Inside edge AWS");
                    System.setProperty("webdriver.edge.driver","C:\\Users\\jjhlsa\\msedgedriver.exe");
                    EdgeOptions options = new EdgeOptions();
                    List<String> args = Arrays.asList("disable-features=msEdgeDevToolsWdpRemoteDebugging", "ie-mode-force");
                    Map<String, Object> map = new HashMap<String, Object>();
                    map.put("args", args);
                    options.setCapability("ms:edgeOptions", map);
                    options.setCapability("excludeSwitches", "remote-debugging-port=0");
                   original = new EdgeDriver(options);
					WebDriverListener CustomListener=new DriverEventListener();
					WebDriver E_driver = new EventFiringDecorator(CustomListener).decorate(original);
					driver=E_driver;
					driver.manage().window().maximize();
					
				}
				else if(StringUtils.isNotBlank(environment) && "Local".equalsIgnoreCase(environment)){
                     
					//WebDriverManager.edgedriver().setup(); 
					System.out.println("Inside Edge local");
					System.setProperty("webdriver.edge.driver","C:\\Users\\jjhlsa\\msedgedriver.exe");
					//System.out.println("Browser executable path is set as :- "+System.getProperty("webdriver.edge.driver"));
					EdgeOptions options = new EdgeOptions();
                    List<String> args = Arrays.asList("disable-features=msEdgeDevToolsWdpRemoteDebugging", "ie-mode-force");
                    Map<String, Object> map = new HashMap<String, Object>();
                    map.put("args", args);
                    options.setCapability("ms:edgeOptions", map);
                    options.setCapability("excludeSwitches", "remote-debugging-port=0");
                    original = new EdgeDriver(options);
					WebDriverListener CustomListener=new DriverEventListener();
					WebDriver E_driver = new EventFiringDecorator(CustomListener).decorate(original);
					driver=E_driver;
					driver.manage().window().maximize();
				}
				else{
					PrintUtil.logError("Environment is null", logger, "EnvironmentNotSet",driver,"FAIL");
				}
			}
			else {
				driver = null;
			
			}
			
			
